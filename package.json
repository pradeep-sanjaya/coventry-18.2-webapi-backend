    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "ejs": "^3.0.2",
    "express": "^4.17.1",
    "express-validator": "^6.2.0",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.9.7",
    "multer": "^1.4.2",
    "rimraf": "^3.0.2",
    "sharp": "^0.25.1",
    "swagger-jsdoc": "^4.0.0",
    "swagger-ui-express": "^4.1.3",
    "uuid": "^7.0.2",
